<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
        xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
    <persistence-unit name="jpars_example" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <!--jta-data-source>jdbc/JPARSStudentDS</jta-data-source-->
        <class>com.oracle.jsonduality.model.Type</class>
        <class>com.oracle.jsonduality.model.Trainer</class>
        <class>com.oracle.jsonduality.model.Pokemon</class>
        <class>com.oracle.jsonduality.model.OwnPokemon</class>
        <exclude-unlisted-classes/>
        <properties>
            <property name="eclipselink.logging.level" value="FINEST"/>
            <property name="eclipselink.logging.parameters" value="true"/>
            <property name="eclipselink.ddl-generation" value="none"/>
            <property name="jakarta.persistence.jdbc.driver" value="oracle.jdbc.driver.OracleDriver"/>
            <property name="jakarta.persistence.jdbc.url" value="${oradb23c.url}"/>
            <property name="jakarta.persistence.jdbc.user" value="${oradb23c.userName}"/>
            <property name="jakarta.persistence.jdbc.password" value="${oradb23c.userPass}"/>
        </properties>
    </persistence-unit>
</persistence>
